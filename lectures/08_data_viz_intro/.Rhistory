library(tidyverse)
starwars |>
filter(species == "Human") |>
ggplot(aes(x = height, y = mass)) +
geom_point() +
labs(title = "Star Wars Characters: Height vs. Mass",
x = "Height (cm)",
y = "Mass (kg)") +
theme_minimal()
